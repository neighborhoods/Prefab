{
  "name": "neighborhoods/prefab",
  "type": "library",
  "description": "Neighborhoods Prefab builds Protean machinery.",
  "license": "MIT",
  "keywords": [],
  "bin": [
    "bin/prefab"
  ],
  "authors": [
    {
      "name": "Noah Leapai",
      "email": "noah.leapai@neighborhoods.com"
    },
    {
      "name": "Jake Malachowski",
      "email": "jacob.malachowski@neighborhoods.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.neighborhoods.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "jsoumelidis/zend-sf-di-config": "^0.3",
    "doctrine/dbal": "^2.7",
    "symfony/config": "^4.0",
    "symfony/console": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/filesystem": "^4.3",
    "symfony/finder": "^4.0",
    "symfony/yaml": "^4.0",
    "zendframework/zend-code": "^3.3",
    "zendframework/zend-component-installer": "^2.1.1",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-diactoros": "^1.7.1",
    "zendframework/zend-expressive": "^3.0.1",
    "zendframework/zend-expressive-fastroute": "^3.0",
    "zendframework/zend-expressive-helpers": "^5.0",
    "zendframework/zend-http": "^2.8",
    "zendframework/zend-problem-details": "^1.0",
    "zendframework/zend-stdlib": "^3.1",
    "neighborhoods/buphalo": "1.0.0-RC4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Neighborhoods\\Prefab\\": [
        "src",
        "fab"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neighborhoods\\PrefabTest\\": "tests",
      "Neighborhoods\\ReplaceThisWithTheNameOfYourProduct\\": "http/fab"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
