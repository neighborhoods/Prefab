{
  "name": "neighborhoods/PROJECTNAME",
  "description": "Add a short description of your project here.",
  "type": "project",
  "license": "proprietary",
  "keywords": [
    "prefab"
  ],
  "scripts": {
    "post-update-cmd": [
      "vendor/bin/prefab",
      "@composer dump-autoload -a",
      "vendor/bin/bakery"
    ],
    "post-install-cmd": [
      "vendor/bin/prefab",
      "@composer dump-autoload -a",
      "vendor/bin/bakery"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.neighborhoods.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.2",
    "neighborhoods/prefab": "^5.11",
    "neighborhoods/bakery": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Neighborhoods\\PROJECTNAME\\": [
        "src",
        "fab"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neighborhoods\\PROJECTNAME\\": [
        "src/",
        "test-fab/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
